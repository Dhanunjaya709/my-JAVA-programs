import java.util.Scanner;
public class Perfect
{
    public static void main(String[] args)
    {
     Scanner sc=new Scanner(System.in);
     int a,b=0;
     a=sc.nextInt();
     int i=1;
     while(i<=a/2)
     {
         if(a%i==0)
         b=b+i;
         i++;
     }
         if(b==a)
         System.out.println("Perfect Number");
         else
         System.out.println("Not a Perfect Number");
         }
     
    }
