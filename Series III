import java.util.Scanner;
public class Series3
{
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        int b;
        for(int i=1;i<=a;i++)
        {
            b=(int)Math.pow(3,i);
            System.out.printf("%d ",b);
        }
        
        
    }
}
