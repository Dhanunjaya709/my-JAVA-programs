import java.util.Scanner;
public class Lcm
{
    public static void main(String[] args)
    {
     Scanner sc=new Scanner(System.in);
     int a=sc.nextInt();
     int b=sc.nextInt();
     int c=1;
     for(int i=1;i<=a&&i<=b;++i)
     {
         if(a%i==0&&b%i==0)
         c=i;
     }
     int d=(a*b)/c;
     System.out.printf("LCM of %d and %d is %d",a,b,d);
     
    }
}
