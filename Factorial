import java.util.Scanner;
public class Factorial
{
    public static void main(String[] args)
    {
       Scanner sc=new Scanner(System.in);
       int a=sc.nextInt();
        int fact=1,i=1;
       while(fact<a)
       {
           i=i+1;
       fact=fact*i;
       }
       if(fact==a)
           System.out.println("yes");
        else
           System.out.println("no");
       
       
      
    }
}
