import java.util.Scanner;
public class Flash
{
    public static void main(String args[])
    {
       Scanner sc=new Scanner(System.in);
       int a,b,c,d,e,f;
       a=sc.nextInt();
       b=sc.nextInt();
       c=sc.nextInt();
       d=sc.nextInt();
       e=sc.nextInt();
       f=sc.nextInt();
       double g=(a+c+e)/3.0;
       double h=(b+d+f)/3.0;
       System.out.printf("%.1f",g);
       System.out.println();
       System.out.printf("%.1f",h);
    }
}
