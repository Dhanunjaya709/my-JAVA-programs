import java.util.Scanner;
public class Flash
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        int b=sc.nextInt();
        int c=sc.nextInt();
        int d=sc.nextInt();
       double e=(a+c);
        double f=(b+d);
        System.out.println(e/2);
        System.out.println(f/2);
    }
}
