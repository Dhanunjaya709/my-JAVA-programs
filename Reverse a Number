import java.util.Scanner;
public class Reverse
{
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        int a,b=0,c=0;
        a=sc.nextInt();
        while(a!=0)
        {
            b=a%10;
            c=(c*10)+b;
            a=a/10;
        }
            System.out.println(c);
    }
}
