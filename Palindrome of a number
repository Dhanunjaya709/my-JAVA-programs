import java.util.Scanner;
public class Palindrome
{
    public static void main(String[] args)
    {
       Scanner sc=new Scanner(System.in);
        int a,b,c=0,d=0;
        b=sc.nextInt();
        int temp=b;
        while(b>0)
        {
            a=b%10;
            d=d*10+a;
            b=b/10;
        }
            if(d==temp)
            System.out.println("Palindrome");
            else
            System.out.println("Not a Palindrome");
        
    }
}
